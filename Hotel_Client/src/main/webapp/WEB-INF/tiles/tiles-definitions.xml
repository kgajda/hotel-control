<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
 
    <definition name="defaultTemplate" template="/WEB-INF/template/template.jsp">
        <put-attribute name="menu" value="/WEB-INF/template/menu.jsp" />
        <put-attribute name="footer" value="/WEB-INF/template/footer.jsp" />
    </definition>
    <definition name="adminTemplate" template = "/WEB-INF/template/admintemplate.jsp">
        <put-attribute name="adminmenu" value="/WEB-INF/template/adminmenu.jsp" />
        <put-attribute name="menu" value="/WEB-INF/template/menu.jsp" />
        <put-attribute name="footer" value="/WEB-INF/template/footer.jsp" />
    </definition>
</tiles-definitions>